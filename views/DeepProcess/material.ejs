<!DOCTYPE html>
<html lang="en">

<head>
    <title>福娃糙米</title>
    <link rel="stylesheet" type="text/css" href="/css/base.css">
</head>

<body>
    <% include ../base %>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">原料管理</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <!-- <div class="panel-heading">
                            DataTables Advanced Tables
                        </div> -->
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>materialid</th>
                                        
                                        <th>date</th>
                                        <th>weight</th>
                                        <th>kind</th>
                                        <th>source</th>
                                        <th>personid</th>
                                    </tr>
                                </thead>
                                <tbody>

                                <% for (var i = 0;i < data.length;i++) {%>
                                    <tr class="gradeA">
                                        <td><%=data[i].materialid%></td>
                                        
                                        <td><%=data[i].date%></td>
                                        <td><%=data[i].weight%></td>
                                        <td><%=data[i].kind%></td>
                                        <td><%=data[i].source%></td>
                                        <td><%=data[i].personid%></td>
                                    </tr>   
                                <% } %>
                                    
                                </tbody>
                            </table>
                  
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <a href="#" class="add">添加</a>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

</body>

</html>

